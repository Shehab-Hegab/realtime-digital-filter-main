}



/* Menu Items */
QMenu {
    background-color: #1F1F1F; /* Slightly Darker Gray */
    color: #FFFFFF; /* White text */
}

/* Menu Item Hover */
QMenu::item:selected {
    background-color: #444444; /* Dark Gray on hover */
}

/* Toolbars */
QToolBar {
    background-color: #1F1F1F; /* Slightly Darker Gray */
    color: #FFFFFF; /* White text */
}

/* Push Buttons */
QPushButton {
    background-color: #0074D9; /* Dark Blue */
    color: #FFFFFF; /* White text */
    border: 2px solid #0056B3; /* Darker Blue border */
    border-radius: 5px; /* Rounded corners */
    padding: 8px 16px; /* Padding around text */
    font-size: 14px; /* Font size */
    font-weight: bold; /* Bold text */
}

/* Push Button Hover */
QPushButton:hover {
    background-color: #0056B3; /* Darker Blue on hover */
}

/* Push Button Pressed */
QPushButton:pressed {
    background-color: #003E7D; /* Even darker blue when pressed */
}

/* Push Button Disabled */
QPushButton:disabled {
    background-color: #DDDDDD; /* Light gray for disabled */
    color: #999999; /* Dimmed text */
    border: 2px solid #CCCCCC; /* Slightly darker border for disabled */
}

/* LineEdits, TextEdits */
QLineEdit, QTextEdit {
    background-color: #1F1F1F; /* Slightly Darker Gray */
    color: #FFFFFF; /* White text */
    border: 2px solid #444444; /* Dark Gray border */
    border-radius: 5px; /* Rounded corners */
    padding: 5px; /* Padding around text */
    font-size: 20px;
}

/* LineEdit and TextEdit Focus */
QLineEdit:focus, QTextEdit:focus {
    border-color: #77A1D3; /* Light Blue border when focused */
}

/* ScrollBars */
QScrollBar:vertical {
    background-color: #1F1F1F; /* Slightly Darker Gray */
    width: 10px; /* Scroll bar width */
    border-radius: 5px; /* Rounded corners */
}

QScrollBar::handle:vertical {
    background-color: #444444; /* Dark Gray scroll bar handle */
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
}

/* Labels */
/* QLabel with Background Color */
QLabel#coloredLabel {
    background-color: #FF5733; /* Orange background */
    color: #FFFFFF; /* White text */
    padding: 5px; /* Padding around text */
    border-radius: 4px; /* Rounded corners */
}

/* QLabel with Border */
QLabel#borderedLabel {
    border: 2px solid #3498DB; /* Border with a blue color */
    padding: 5px; /* Padding around text */
    border-radius: 4px; /* Rounded corners */
}

/* QLabel with Custom Font */
QLabel#customFontLabel {
    font-family: &quot;Arial&quot;; /* Custom font family */
    font-size: 18px; /* Custom font size */
    font-weight: bold; /* Bold text */
}

/* QLabel with Text Shadow */
QLabel#shadowLabel {
    text-shadow: 2px 2px 4px #333333; /* Text shadow with offset and color */
}

/* QLabel with Underline */
QLabel#underlinedLabel {
    text-decoration: underline; /* Underline text */
}

/* QLabel with Strike Through */
QLabel#strikeThroughLabel {
    text-decoration: line-through; /* Strike through text */
}

/* QLabel with Text Alignment */
QLabel#centeredLabel {
    text-align: center; /* Center-align text */
}

/* QLabel with Text Color Gradient */
QLabel#gradientLabel {
    background: linear-gradient(to right, #3498DB, #FF5733); /* Text color gradient */
    -webkit-background-clip: text; /* Apply gradient to text */
    color: transparent; /* Hide text color */
}











